          "editorMode": "code",
          "format": "time_series",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  $__timeGroup(Datetime,$__interval) AS \"time\",\n  sum(mons_seen) AS 'Mons scanned',\n  sum(mons_enc) AS 'Mons encountered',\n  100*sum(mons_enc)/sum(mons_seen) AS '%IV',\n  sum(loc_time)/sum(loc_success) AS \"Location handling time\",\n  sum(loc_count) AS \"Locations handled\",\n  100*sum(loc_success)/sum(loc_count) AS \"Location success rate\",\n  100*sum(numRep)/(sum(rpl)/5) AS \"% raw data received\",\n  sum(rpl)*60 - sum(loc_time) AS \"lost scanning time\"\nFROM stats_worker\nWHERE\n  $__timeFilter(Datetime) and\n  RPL='$RPL' and\n  controller_worker in ($controller_worker) and\n  (\n  left(device_worker,17) in ($device) OR \n  (rpl=1440 and device_worker is NULL)\n  )\nGROUP BY 1  \nORDER BY $__timeGroup(Datetime,$__interval)",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "id"
                ],
                "type": "column"
              }
            ]
          ],
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "stats_worker",
          "timeColumn": "Datetime",
          "timeColumnType": "datetime",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
